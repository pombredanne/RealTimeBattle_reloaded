#: src/RealTimeBattle.cc:91
msgid " Usage: RealTimeBattle [options] "
msgstr " Användning: RealTimeBattle [flaggor] "

#: src/RealTimeBattle.cc:92
msgid " Options:"
msgstr " Flaggor:"

#: src/RealTimeBattle.cc:93
msgid "    --debug_mode,                -d   debug mode"
msgstr "    --debug_mode,                -d   debugläge"

#: src/RealTimeBattle.cc:94
msgid ""
"    --debug_level [0-5],         -D   sets the initial debug level. Implies "
"-d"
msgstr "    --debug_level [0-5],         -D   sätt debug-nivå. Implicerar -d"

#: src/RealTimeBattle.cc:95
msgid "    --normal_mode,               -n   normal mode (default)"
msgstr "    --normal_mode,               -n   normalt läge"

#: src/RealTimeBattle.cc:96
msgid "    --competition_mode,          -c   competition mode"
msgstr "    --competition_mode,          -c   tävlingsläge"

#: src/RealTimeBattle.cc:98
msgid "    --no_graphics,               -g   no graphics will be displayed"
msgstr "    --no_graphics,               -g   visa ingen grafik"

#: src/RealTimeBattle.cc:99
msgid ""
"    --option_file [file],        -o   selects option-file (default: "
"$HOME/.rtbrc)"
msgstr ""
"    --option_file [fil],         -o   välj inställningsfil (normalt: "
"$HOME/.rtbrc)"

#: src/RealTimeBattle.cc:101
msgid ""
"    --log_file [file],           -l   make log file, if 'file' is '-'\n"
"                                      the log is sent to STDOUT"
msgstr ""
"    --log_file [fil],            -l   skapa log-file, om 'fil' är '-'\n"
"                                      så skickas loggen till STDOUT"

#: src/RealTimeBattle.cc:104
msgid ""
"    --tournament_file [file],    -t   specify a tournament file to\n"
"                                      autostart a tournament"
msgstr ""
"    --tournament_file [fil],     -t   välj turneringsfil för automatisk\n"
"                                      start av turneringen"

#: src/RealTimeBattle.cc:107
msgid ""
"    --statistics_file [file],    -s   file to print the statistics to\n"
"                                       when autostarting"
msgstr ""
"    --statistics_file [fil],     -s   skriv statistik till fil vid\n"
"                                      automatisk turneringsstart"

#: src/RealTimeBattle.cc:110
msgid ""
"    --message_file [file],       -m   redirect messages to 'file'.\n"
"                                      With '-' as 'file', STDOUT is used.\n"
"                                      If both log and messages are sent\n"
"                                      to STDOUT, '-m' will be ignored"
msgstr ""
"    --message_file [fil],        -m   skicka meddelanden till 'fil'.\n"
"                                      Om 'fil' är '-' används STDOUT.\n"
"                                      Om både loggen och meddelanden "
"skickas\n"
"                                      till STDOUT kommer '-m' att ignoreras"

#: src/RealTimeBattle.cc:115
msgid ""
"    --replay [file]              -r   a log file to replay.\n"
"                                      if '-' is specified as file,\n"
"                                      input is taken from STDIN"
msgstr ""
"    --replay [fil]               -r   visa spel sparat i 'fil'.\n"
"                                      om filen är '-' kommer\n"
"                                      STDIN användas"

#: src/RealTimeBattle.cc:119
msgid "    --help,                      -h   prints this message"
msgstr "    --help,                      -h   skriv ut denna hjälptext"

#: src/RealTimeBattle.cc:120
msgid "    --version,                   -v   prints the version number"
msgstr "    --version,                   -v   visa versionsnummer"

